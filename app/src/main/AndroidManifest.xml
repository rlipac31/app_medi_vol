            android:name=".model.medico.EditMedicoActivity"
            android:exported="false" />
        <activity
            android:name=".formularios.RegistroPacienteActivity"
            android:exported="false" />
        <activity
            android:name=".formularios.RegistroMedicoActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".PacienteActivity"
            android:exported="false" />
        <activity
            android:name=".ConsultaActivity"
            android:exported="false" />
        <activity
            android:name=".MedicoActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuPrincipalActivity"
            android:exported="false" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
